{
  "cacheDirectory": ".jestcache/",
  "testRegex": "__tests__\/.*test.jsx?$",
  "transform": {
    ".*": "<rootDir>/node_modules/babel-jest"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "setupFiles": [
    "<rootDir>/test-setup.js",
    "jest-canvas-mock"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/__tests__/__mocks__/",
    "<rootDir>/__tests__/commands.jsx",
    "<rootDir>/src/animations/",
    "<rootDir>/assets/",
    "<rootDir>/src/config/",
    "<rootDir>/src/projects/",
    "<rootDir>/src/scss/"
  ],
  "moduleNameMapper": {
    "^config$": "<rootDir>/src/config/jest-app-config.js",
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/__mocks__/fileMock.js",
    "\\.(css|scss)$": "identity-obj-proxy"
  },
  "testURL": "http://localhost"
}